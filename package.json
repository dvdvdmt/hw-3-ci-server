{
  "name": "hw-3-ci-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/dvdvdmt/hw-3-ci-server.git",
  "scripts": {
    "api:generate": "bash openapi/generate-api-client.sh",
    "build:client:dev": "webpack --config client/webpack/webpack.config.js --env.dev",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "lint:js": "eslint ./client ./server ./cypress --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint client/src/**/*.scss",
    "lint:style:fix": "npm run lint:style -- --fix",
    "start": "npm run start:server:dev",
    "start:client:dev": "webpack-dev-server --config client/webpack/webpack.config.js --env.dev --watch",
    "start:server": "node server/server.js",
    "start:server:dev": "nodemon --watch server server/server.js",
    "test:unit": "jest",
    "test:e2e": "cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.scss": "stylelint --fix"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "cypress"
    ]
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.12-4.3.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.11.0",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "morgan": "^1.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-portal": "^4.2.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "superagent": "^5.2.2",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@types/express": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cypress": "^4.3.0",
    "cypress-dotenv": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.2",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0",
    "svg-sprite-loader": "^4.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
